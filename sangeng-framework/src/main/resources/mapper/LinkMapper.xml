<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaohe.mapper.LinkMapper">

    <select id="getAllLinks" resultType="com.xiaohe.domain.vo.LinkVo">
        select name, logo, description, address
        from link
        where status = 0 and del_flag = 0;
    </select>
</mapper>